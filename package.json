{
  "name": "wildlife-react-redux-boilerplate",
  "version": "1.0.0",
  "description": "React/Redux SPA Build Server",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf public/assets/* public/*.html",
    "dev": "npm run clean && npm run dev:vendors && concurrently  --kill-others \"npm run dev:template\"  \"NODE_ENV=development node ./config/webpack.dev.server\"",
    "dev:vendors": "NODE_ENV=development  webpack --config ./config/webpack.config.dll.js",
    "dev:template": "pug src/pug/index.pug --watch --out public/",
    "build": "npm run clean && npm run build:template && npm run build:vendors && NODE_ENV=production webpack --config ./config/webpack.config.prod.js",
    "build:vendors": "NODE_ENV=production webpack --config ./config/webpack.config.dll.js",
    "build:template": "NODE_ENV=production pug src/pug/index.pug --out public/"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate",
    "hmr",
    "webpack"
  ],
  "author": "WILDLIFE.LA",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "6.3.26",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^2.2.2",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.3.13",
    "babel-runtime": "^6.2.0",
    "css-loader": "^0.23.0",
    "envify": "^3.4.0",
    "eslint": "^2.7.0",
    "eslint-config-rackt": "^1.1.1",
    "eslint-config-standard-react": "^2.3.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jsx-loader": "^0.13.2",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.0",
    "precss": "^1.4.0",
