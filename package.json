{
  "name": "react-redux-boilerplate-deploy",
  "version": "0.0.1",
  "description": "React/Redux SPA Build Server with S3 Deployment Tasks",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development node index.js",
    "windev": "set NODE_ENV=development && node ./index.js",
    "build:template": "jade -O env.json server/view/index.jade --out public/",
    "build": "npm run build:template && NODE_ENV=production webpack --config ./server/webpack/webpack.config.prod.js",
    "deploy": "npm run build && gulp s3"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate",
    "hmr",
    "webpack",
    "s3",
    "deployment"
  ],
